<h1>Cadastre-se</h1>

<a href="'/usuarios/novo"> Cadastrar Usuario</a>
<table border="1">
 <tr>
    <td>ID</td>
    <td>NickName</td>
    <td>Nome</td>
 </tr>
 {{#each usuarios}}
    <tr>
        <td>{{this.id}}</td>
        <td>{{this.nickname}}</td>
        <td>{{this.nome}}</td>
        <td>
            <a href="/usuarios/{{this.id}}/atualizar">Atualizar</a>
            <form action="/usuarios/excluir" method="post">
                <input type="text" value="{{this.id}}">
                <input type="submit" value="Excluir">
            </form>
        </td>
    </tr>
 {{/each}}
</table> 
